<%= simple_form_for @list do |f| %>
  <div class="form-group">
    <%= f.input :name %>
  </div>

    <div class="form-group">
      <%= f.input :category, collection: List::CATEGORIES, prompt: "Choose a category" %>
  </div>

  <%= f.button :submit, "Create", class: "btn btn-primary" %>
  <%= link_to "Back", lists_path, class: "btn btn-secondary" %>
<% end %>
